{{> navbarLoggedAdmin }}
{{!-- BOOTSTRAP REGISTER --}}
<link href="/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
<link href="/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
<!-- Font special for pages-->
<link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i"
    rel="stylesheet">

<!-- Vendor CSS-->
<link href="/vendor/select2/select2.min.css" rel="stylesheet" media="all">
<link href="/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">

<!-- Main CSS-->
<div class="col-md-6 offset-md-3">
    <div class="card">
        <div class="card-header">
            <h1>Todos los dietistas registrados</h1>
        </div>

            <div class="card-body">
            {{#if dietician}}

            <div class="slds-scrollable_y-ManejarPaciente scroll-ManejarPaciente">
                {{#each dietician}}
                <!-- <lightning-card class="carta"> -->

                <div class="card-Paciente-ManejarPaciente">
                    <div class="columnaFilas-ManejarPaciente">


                        <div class="filaDatos-ManejarPaciente">
                            <div class="columnaDatos-ManejarPaciente">
                                <p class="notas-text-primera-ManejarPaciente">Nombre y apellidos:</br> <span
                                        class="notas-text-abajo-ManejarPaciente">{{username}}</span></p>

                            </div>
                            <div class=" columnainfoCorta-ManejarPaciente">
                                <p class="notas-text-primera-ManejarPaciente ">Email: </br><span
                                        class="notas-text-abajo-ManejarPaciente">{{email}}</span>
                                </p>
                            </div>
                            <div class=" columnainfoCorta-ManejarPaciente">
                                <p class="notas-text-primera-ManejarPaciente ">Estado: </br><span
                                        class="notas-text-abajo-ManejarPaciente">{{status}}</span></p>
                            </div>

                            
                        </div>
                        <div class="filaDatos-ManejarPaciente">
                            <div class="columnaDatos-ManejarPaciente">
                                <p class="notas-text-primera-ManejarPaciente ">Descripción:</br> <span
                                        class="notas-text-abajo-ManejarPaciente">{{description}}</span>
                                </p>

                            </div>
                            <div class=" columnainfoCorta-ManejarPaciente">
                                <p class="notas-text-primera-ManejarPaciente ">Teléfono:</br> <span
                                        class="notas-text-abajo-ManejarPaciente">{{phone}}</span></p>
                            </div>
                            <div class="columnainfoCorta-ManejarPaciente">
                                <p class="notas-text-primera-ManejarPaciente ">Valoración:</br> <span
                                        class="notas-text-abajo-ManejarPaciente">{{worth}}</span></p>

                            </div>
                            
                        </div>
                        
                    </div>

                    <div class="columnaBotones-ManejarPaciente">
                         <a href="/modify-dietician/{{@key}}" class="btn myButtonVerde botonDietistaAdmin">Modificar dietista</a>                   
                    <a href="/delete-dietician/{{@key}}" class="btn myButtonRojo">Borrar dietista</a>
                    </div>


                  


                </div>

                <!-- </lightning-card> -->


                {{/each}}
            </div>






            {{else}}
            <p>No tienes pacientes asignados</p>
            {{/if}}
        </div>

    </div>
</div>

<!-- Jquery JS-->
<script src="/vendor/jquery/jquery.min.js"></script>
<!-- Vendor JS-->
<script src="/vendor/select2/select2.min.js"></script>
<script src="/vendor/datepicker/moment.min.js"></script>
<script src="/vendor/datepicker/daterangepicker.js"></script>
<!-- Main JS-->
<script src="/js/global.js"></script