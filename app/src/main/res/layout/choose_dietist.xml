<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="109dp">

        <Space
            android:layout_width="match_parent"
            android:layout_height="55dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:gravity="center"
            android:text="@string/ya_casi_est_s_elige_tu_dietista" />

        <Spinner
            android:id="@+id/spinnerDietician"
            android:layout_width="match_parent"
            android:layout_height="45dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/nombreDietician"
                android:layout_width="match_parent"
                android:layout_height="59dp"
                android:gravity="center" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="33dp" />

            <TextView
                android:id="@+id/valoracionDietician"
                android:layout_width="match_parent"
                android:layout_height="47dp"
                android:gravity="center" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="44dp" />

            <TextView
                android:id="@+id/comentarioDietician"
                android:layout_width="match_parent"
                android:layout_height="51dp"
                android:gravity="center" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="228dp" />

            <Button
                android:id="@+id/button3"
                android:layout_width="match_parent"
                android:layout_height="65dp"
                android:onClick="confirmChooseDietician"
                android:text="@string/confirmar"
                android:theme="@style/AppTheme" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>